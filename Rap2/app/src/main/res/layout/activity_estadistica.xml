<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_estadistica"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.pregrado.triviapp.Activity_Estadistica">

    <TextView
        android:text="Estadisticas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="125dp"
        android:layout_marginStart="125dp"
        android:layout_marginTop="35dp"
        android:id="@+id/textView7" />

    <TextView
        android:text="P1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView7"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="14dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="33dp"
        android:id="@+id/P1" />

    <TextView
        android:text="P2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="29dp"
        android:id="@+id/P2"
        android:layout_below="@+id/P1"
        android:layout_alignLeft="@+id/P1"
        android:layout_alignStart="@+id/P1" />

    <TextView
        android:text="P3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="22dp"
        android:id="@+id/P3"
        android:layout_below="@+id/P2"
        android:layout_alignLeft="@+id/P2"
        android:layout_alignStart="@+id/P2" />

    <TextView
        android:text="P4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:id="@+id/P4"
        android:layout_below="@+id/P3"
        android:layout_alignLeft="@+id/P3"
        android:layout_alignStart="@+id/P3" />

    <TextView
        android:text="P5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/P4"
        android:layout_alignLeft="@+id/P4"
        android:layout_alignStart="@+id/P4"
        android:layout_marginTop="14dp"
        android:id="@+id/P5" />

    <TextView
        android:text="5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/P1"
        android:layout_toRightOf="@+id/P1"
        android:layout_toEndOf="@+id/P1"
        android:layout_marginLeft="46dp"
        android:layout_marginStart="46dp"
        android:id="@+id/p1C" />

    <TextView
        android:text="2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/p1C"
        android:layout_alignRight="@+id/textView7"
        android:layout_alignEnd="@+id/textView7"
        android:id="@+id/p1M" />

    <TextView
        android:text="2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/P2"
        android:layout_toEndOf="@+id/P2"
        android:id="@+id/p2C"
        android:layout_alignLeft="@+id/p1C"
        android:layout_alignStart="@+id/p1C" />

    <TextView
        android:text="2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/p2C"
        android:id="@+id/p2M"
        android:layout_alignLeft="@+id/p1M"
        android:layout_alignStart="@+id/p1M" />

    <TextView
        android:text="4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/p3C"
        android:layout_alignBaseline="@+id/P3"
        android:layout_alignBottom="@+id/P3"
        android:layout_alignLeft="@+id/p2C"
        android:layout_alignStart="@+id/p2C" />

    <TextView
        android:text="5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/p3M"
        android:layout_alignBaseline="@+id/p3C"
        android:layout_alignBottom="@+id/p3C"
        android:layout_alignLeft="@+id/p2M"
        android:layout_alignStart="@+id/p2M" />

    <TextView
        android:text="1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/p4C"
        android:layout_alignBaseline="@+id/P4"
        android:layout_alignBottom="@+id/P4"
        android:layout_alignLeft="@+id/p3C"
        android:layout_alignStart="@+id/p3C" />

    <TextView
        android:text="8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/p4M"
        android:layout_alignBaseline="@+id/p4C"
        android:layout_alignBottom="@+id/p4C"
        android:layout_alignLeft="@+id/p3M"
        android:layout_alignStart="@+id/p3M" />

    <TextView
        android:text="9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/p5C"
        android:layout_alignBaseline="@+id/P5"
        android:layout_alignBottom="@+id/P5"
        android:layout_alignLeft="@+id/p4C"
        android:layout_alignStart="@+id/p4C" />

    <TextView
        android:text="5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/p5M"
        android:layout_alignBaseline="@+id/p5C"
        android:layout_alignBottom="@+id/p5C"
        android:layout_alignLeft="@+id/p4M"
        android:layout_alignStart="@+id/p4M" />

</RelativeLayout>
